<Window x:Class="BioLab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BioLab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="900"
        Loaded="Window_Loaded">
    
    <Grid x:Name="RootContainer">
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="HeaderContainer" Height="60" Grid.Row="0" Grid.Column="0" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="BodyContainer" Grid.Row="1" Grid.Column="0" NavigationUIVisibility="Hidden"/>
        <Frame x:Name="FooterContainer" Height="60" Grid.Row="2" Grid.Column="0" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
